{
  "name": "size-limit-action",
  "version": "1.5.1",
  "private": true,
  "description": "size-limit action",
  "main": "dist/index.js",
  "license": "ISC",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "build": "ncc build src/main.ts -o dist",
    "size-build": "npm run build",
    "size": "npm run size-build && size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "4500 ms"
    }
  ],
  "dependencies": {
    "@actions/artifact": "^0.6.1",
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0",
    "@actions/glob": "^0.2.0",
    "bytes": "^3.1.1",
    "github-fetch-workflow-artifact": "2.0.0",
    "has-pnpm": "^1.1.1",
    "has-yarn": "^3.0.0",
    "markdown-table": "^3.0.2"
  },
  "devDependencies": {
    "@size-limit/file": "^7.0.5",
    "@size-limit/time": "^7.0.5",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^5.10.1",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.7.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.0.0",
    "jest": "^27.4.7",
    "jest-circus": "^27.4.6",
    "prettier": "^2.5.1",
    "size-limit": "^7.0.5",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
